{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/docs",
            "dest": "/api/index.py"
        },
        {
            "src": "/docs-page",
            "dest": "/api/index.py"
        },
        {
            "src": "/predictions",
            "dest": "/api/index.py"
        },
        {
            "src": "/model-info",
            "dest": "/api/index.py"
        },
        {
            "src": "/upload",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
} 